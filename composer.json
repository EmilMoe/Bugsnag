{
    "name": "emilmoe/bugsnag",
    "type": "extension",
    "description": "Bugsnag Laravel wrapper adds autoloader and other new Laravel features that isn't included already.",
    "license": "MIT",
    "keywords": [
        "Bugsnag"
    ],
    "authors": [{
        "name": "Emil Moe",
        "email": "emil@moegroup.dk",
        "homepage": "https://emilmoe.com"
    }],
    "require": {
        "illuminate/support": "^5.0",
        "php": ">=5.5",
        "bugsnag/bugsnag-laravel": "^2.0",
        "bugsnag/bugsnag-js": "^6.0",
        "oomphinc/composer-installer-extender": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^6.0|^7.0"
    },
    "repositories": [
        {"type": "vcs", "url": "git@github.com:bugsnag/bugsnag-js.git"}
    ],
    "autoload": {
        "psr-4": {
            "EmilMoe\\Bugsnag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilMoe\\Bugsnag\\BugsnagServiceProvider",
                "Bugsnag\\BugsnagLaravel\\BugsnagServiceProvider"
            ]
        },
        "installer-types": ["npm-package"],
        "installer-paths": {
            "docroot/node_modules/{$name}": ["type:npm-package"]
        }
    }
}